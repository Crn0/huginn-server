{
  "name": "hello-prisma",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node ./dist/src/bin/www.js",
    "dev": "tsx watch ./src/bin/www.ts",
    "server": "DEBUG=app:* npm run dev",
    "prisma:init": "npx prisma init",
    "prisma:pull": "npx prisma pull",
    "prisma:validate": "npx prisma validate",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:createOnly": "npx prisma migrate dev --create-only",
    "prisma:generate": "npx prisma generate",
    "prisma:sql": "npx prisma generate --sql",
    "prisma:sql:watch": "npx prisma generate --sql --watch",
    "type:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{cjs,mjs,js,ts}\"",
    "test": "NODE_ENV=test vitest"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "argon2": "^0.44.0",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
